{
  "name": "restapi",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/serve.js",
  "type": "module", 
  "scripts": {
    "start": "nodemon --exec ts-node src/serve.ts",
    "start-build": "nodemon ./build/serve.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run compile-ts",
    "cbs": "npm run build && npm run start",
    "dev": "npm run clean && npm run compile-ts-watch-mode & npm run start",
    "clean": "rm -rf build",
    "compile-ts": "tsc --project ./",
    "compile-ts-watch-mode": "tsc -w --project ./",
    "lint": "eslint . --ext .ts",
    "create-migration": "npx knex migrate:make",
    "migrate-up": "npx knex migrate:latest",
    "migrate-down": "npx knex migrate:rollback"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.13",
    "@types/pg": "^8.11.6",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "winston": "^3.13.0"
  }
}
